   "source": [
    "def wrapper(func):\n",
    "    def inner(*args, **kwargs):\n",
    "        print(\"I AM WRAPPER\" + kwargs[\"text\"])\n",
    "        func(*args, **kwargs)\n",
    "        print(\"AFTER WRAPPER\")\n",
    "    return inner\n",
    "\n",
    "@wrapper\n",
    "def function(*, text):\n",
    "    print(\"Hello, World!\" + text)\n",
    "\n",
    "function(text=\" CHungus\")"
   ]
  }
